{
  "name": "@reuvennaor85/israel-statistics-mcp",
  "version": "0.0.1",
  "description": "Israel Statistics MCP",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "publishConfig": {
    "access": "restricted"
  },
  "license": "MIT",
  "author": {
    "name": "reuvenaor",
    "url": "https://reuvenaor.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reuvenaor/israel-statistics-mcp.git",
    "directory": "israel-statistics-mcp"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "israel",
    "statistics",
    "israel-statistics-mcp",
    "israel-statistics"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./registry": {
      "types": "./dist/registry/index.d.ts",
      "default": "./dist/mcp/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "default": "./dist/mcp/index.js"
    }
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "lint": "eslint src/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts --ext .ts --fix",
    "test": "vitest run"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@babel/parser": "^7.22.6",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "https-proxy-agent": "^6.2.0",
    "node-fetch": "^3.3.0",
    "stringify-object": "^5.0.0",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@types/babel__core": "^7.20.1",
    "@types/node": "^20.14.8",
    "@types/stringify-object": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^6.7.0",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}